function salida=selecciona1(v,n)

% se le a√±ade el vector de ID i saca todas las que acaben en el numero n

L=length(v);
f=1;

for i=2:L-1
    S=v(i,1);
    S=char(S);
    N=length(S);
    if strcmp(n, S(N));
        
       if f==1
           f=2;
           salida=v(i,:);
       else
           salida=[salida;v(i,:)];
       end
    end
end
end
